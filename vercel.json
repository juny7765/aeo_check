{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "aeo-audit-app/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/aeo-audit-app/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/aeo-audit-app/index.html"
        }
    ]
}